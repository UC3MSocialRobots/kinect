<launch>
<arg name="robot"/>
<group ns="$(arg robot)">
  <node pkg="image_transport" type="republish" name="decompress_rgb"
        output="screen"  args="compressed        in:=/$(arg robot)/camera/rgb/image
                               raw               out:=rgb"/>

  <!--
  Compressed version:
  rostopic bw /camera/rgb/image/compressed
  Decompressed version:
  rostopic bw /rgb
  -->
</group>
</launch>
