<launch>
<arg name="robot"/>
<group ns="$(arg robot)">
  <node pkg="image_transport" type="republish" name="decompress_user"
        output="screen"  args="compressed        in:=/$(arg robot)/camera/user/image
                               raw               out:=user"/>

  <!--
  Compressed version:
  rostopic bw /camera/user/image/compressed
  Decompressed version:
  rostopic bw /user
  -->
</group>
</launch>
